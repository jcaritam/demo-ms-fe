{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://admin-postulante-dev.utpxpedition.com" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" }
      ]
    }
  ],
  "routes": [
    {
      "src": "^/(|index\\.html)$",
      "has": [
        { "type": "header", "key": "sec-fetch-dest", "value": "iframe" },
        { "type": "header", "key": "referer", "value": "^https://admin-postulante-dev\\.utpxpedition\\.com(/|$)" }
      ],
      "dest": "/index.html"
    },
    {
      "src": "^/(|index\\.html)$",
      "status": 403,
      "dest": "/403.html"
    },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
